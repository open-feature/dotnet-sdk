{
  "packages": {
    ".": {
      "release-type": "simple",
      "monorepo-tags": false,
      "include-component-in-tag": false,
      "versioning": "default",
      "extra-files": [
        "build/Common.prod.props",
        "README.md"
      ]
    }
  },
  "changelog-sections": [
      {
        "type": "fix",
        "section": "🐛 Bug Fixes"
      },
      {
        "type": "feat",
        "section": "✨ New Features"
      },
      {
        "type": "chore",
        "section": "🧹 Chore"
      },
      {
        "type": "docs",
        "section": "📚 Documentation"
      },
      {
        "type": "perf",
        "section": "🚀 Performance"
      },
      {
        "type": "build",
        "hidden": true,
        "section": "🛠️ Build"
      },
      {
        "type": "deps",
        "section": "📦 Dependencies"
      },
      {
        "type": "ci",
        "hidden": true,
        "section": "🚦 CI"
      },
      {
        "type": "refactor",
        "section": "🔄 Refactoring"
      },
      {
        "type": "revert",
        "section": "🔙 Reverts"
      },
      {
        "type": "style",
        "hidden": true,
        "section": "🎨 Styling"
      },
      {
        "type": "test",
        "hidden": true,
        "section": "🧪 Tests"
      }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
